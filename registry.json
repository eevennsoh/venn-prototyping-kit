{
	"$schema": "https://ui.shadcn.com/schema/registry.json",
	"name": "venn-prototyping-kit",
	"homepage": "https://registry-test-three.vercel.app",
	"items": [
		{
			"name": "setup",
			"type": "registry:block",
			"title": "Prototype Starter Kit",
			"description": "A blank application with all brand components and code",
			"registryDependencies": [
				"https://registry-test-three.vercel.app/r/instructions.json",
				"https://registry-test-three.vercel.app/r/theme.json",
				"https://registry-test-three.vercel.app/r/icon-wrapper.json",
				"https://registry-test-three.vercel.app/r/tokens-mapping.json"
			],
			"files": [
				{
					"path": "registry/ads/blocks/setup/setup-layout.tsx",
					"type": "registry:file",
					"target": "app/layout.tsx"
				},
				{
					"path": "registry/ads/blocks/setup/setup-page.tsx",
					"type": "registry:page",
					"target": "app/page.tsx"
				}
			]
		},
		{
			"name": "ads-style",
			"type": "registry:style",
			"title": "Atlassian Design System Styles",
			"description": "Design tokens from Atlassian Design System",
			"files": [
				{
					"path": "app/tokens.css",
					"type": "registry:file",
					"target": "app/tokens.css"
				}
			]
		},
		{
			"name": "theme",
			"type": "registry:theme",
			"title": "Global Theme",
			"description": "Atlassian Design System theme using Tailwind and Shadcn/ui",
			"files": [
				{
					"path": "app/tokens.css",
					"type": "registry:file",
					"target": "app/tokens.css"
				},
				{
					"path": "app/globals.css",
					"type": "registry:file",
					"target": "app/globals.css"
				}
			]
		},
		{
			"name": "icon-wrapper",
			"type": "registry:lib",
			"title": "Icon Wrapper for Atlassian Design System Icons",
			"description": "A wrapper component for Atlaskit icons",
			"files": [
				{
					"path": "registry/ads/lib/icon-wrapper.tsx",
					"type": "registry:lib"
				}
			]
		},
		{
			"name": "logo-wrapper",
			"type": "registry:lib",
			"title": "Logo Wrapper for Atlassian Design System Logos",
			"description": "A wrapper component for Atlaskit product logos",
			"files": [
				{
					"path": "registry/ads/lib/logo-wrapper.tsx",
					"type": "registry:lib"
				}
			]
		},
		{
			"name": "instructions",
			"type": "registry:file",
			"title": "Instructions",
			"description": "Instructions for the application.",
			"files": [
				{
					"path": "registry/ads/instructions.md",
					"type": "registry:file",
					"target": "app/knowledge/instructions.md"
				}
			]
		},
		{
			"name": "tokens-mapping",
			"type": "registry:file",
			"title": "Tokens Mapping",
			"description": "Full list of Atlassian Design System design tokens and their corresponding Tailwind classes",
			"files": [
				{
					"path": "registry/ads/tokens-mapping.md",
					"type": "registry:file",
					"target": "app/knowledge/tokens-mapping.md"
				}
			]
		},
		{
			"name": "navigation",
			"type": "registry:component",
			"title": "Top Navigation Component",
			"description": "A header navigation bar using ADS tokens and shadcn/ui",
			"registryDependencies": [
				"https://registry-test-three.vercel.app/r/theme.json",
				"https://registry-test-three.vercel.app/r/icon-wrapper.json",
				"https://registry-test-three.vercel.app/r/logo-wrapper.json"
			],
			"files": [
				{
					"path": "registry/ads/components/navigation.tsx",
					"type": "registry:component"
				}
			]
		}
	]
}
